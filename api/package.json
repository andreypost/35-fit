{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "start:dev": "nodemon --exec ts-node src/server.ts",
    "start:dev:inspect": "nodemon --exec node --inspect -r ts-node/register src/server.ts",
    "test": "jest",
    "typeorm": "npm run build && npx typeorm -d dist/config/database.js",
    "typeorm:generate": "npm run typeorm -- migration:generate",
    "typeorm:show": "npm run typeorm migration:show",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:drop": "npm run typeorm schema:drop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "graphql": "^16.9.0",
    "graphql-http": "^1.22.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
