  },\n  {\n    path: COACH_ROUTE,\n    Component: Coach,\n  },\n]\n\nexport const privatRoutes = [\n  {\n    path: FLOWER_ROUTE,\n    Component: Flower,\n  },\n  {\n    path: CHAT_ROUTE,\n    Component: Chat,\n  },\n]\n","/Users/andrii/projects/35-fit/src/store/root.reducer.ts",[],"/Users/andrii/projects/35-fit/src/store/root.store.ts",[],"/Users/andrii/projects/35-fit/src/types/declarations.d.ts",[],"/Users/andrii/projects/35-fit/src/types/interface.ts",[],"/Users/andrii/projects/35-fit/src/utils/action.slice.ts",[],"/Users/andrii/projects/35-fit/src/utils/hooks.ts",["175"],"import React, { useEffect, useState } from 'react'\nimport { TypedUseSelectorHook, useDispatch, useSelector } from 'react-redux'\nimport type { RootState } from 'reducer'\nimport type { AppDispatch } from 'store'\n\nexport const useAppDispatch = () => useDispatch<AppDispatch>()\nexport const useAppSelector: TypedUseSelectorHook<RootState> = useSelector\n\nexport const GetCurrentWindowScroll = () => {\n  const [winScroll, getWinScroll] = useState(0)\n  const setState = () => {\n    getWinScroll(document.body.scrollTop || document.documentElement.scrollTop)\n  }\n  useEffect(() => {\n    window.addEventListener('scroll', setState)\n    return () => window.removeEventListener('scroll', setState)\n  }, [])\n  return winScroll\n}\n","/Users/andrii/projects/35-fit/src/utils/modal.slice.ts",[],"/Users/andrii/projects/35-fit/src/utils/routes.constants.ts",[],"/Users/andrii/projects/35-fit/src/AppRouter.tsx",["176","177","178"],"import React, {\n  createContext,\n  useContext,\n  useEffect,\n  useMemo,\n  useState,\n  useDebugValue,\n} from 'react'\n\nimport {\n  HashRouter,\n  // BrowserRouter,\n  Route,\n  Switch,\n  Redirect,\n} from 'react-router-dom'\nimport { FirebaseAuthContext } from './index'\nimport { useTranslation } from 'react-i18next'\nimport { privatRoutes, publicRoutes } from './routes'\nimport { MAIN_ROUTE } from 'utils/routes.constants'\nimport { HeaderNavigate } from 'components/HeaderNavigate'\nimport { Footer } from 'components/Footer'\nimport { MenuModal } from 'modals/MenuModal'\nimport { LoginModal } from 'modals/LoginModal'\nimport { MessageModal } from 'modals/MessageModal'\nimport { DashboardModal } from 'modals/DashboardModal'\nimport { ACProps } from 'types/interface'\nimport { GetCurrentWindowScroll } from 'utils/hooks'\n// import { createBrowserHistory } from 'history'\n// const history = createBrowserHistory()\n// console.log(history.location.pathname)\n\nexport const AppContext = createContext({} as ACProps)\n\nconst AppRouter = () => {\n  const { user, login, firebaseAuth } = useContext(FirebaseAuthContext),\n    { i18n } = useTranslation(),\n    [language, setLanguage] = useState(i18n.language),\n    winScroll = GetCurrentWindowScroll(),\n    [footerContent, setFooterContent] = useState(false)\n\n  // console.log(\n  //   [54,64,60,59,57,67,62,60].reduce((sum, curr) => sum + curr,0),\n  //   [57,45,45,48,48,47,52,47].reduce((sum, curr) => sum + curr,0)\n  // )\n\n  useEffect(() => {\n    window.onstorage = (e: StorageEvent) => {\n      if (e.key === 'i18nextLng' && e.newValue) {\n        setLanguage(e.newValue)\n        i18n.changeLanguage(e.newValue)\n      }\n    }\n  }, [])\n\n  useEffect(() => {\n    winScroll > 800 && setFooterContent(true)\n  }, [winScroll])\n\n  useDebugValue(user)\n\n  return (\n    <AppContext.Provider value={{ language, setLanguage }}>\n      <HashRouter basename=\"/\">\n        {/* <BrowserRouter basename=\"/\"> */}\n        {useMemo(\n          () => (\n            <HeaderNavigate user={user} />\n          ),\n          [user]\n        )}\n        <Switch>\n          {publicRoutes.map(({ path, Component }) => (\n            <Route\n              key={path}\n              path={path}\n              exact={true}\n              // component={Component}\n            >\n              <Component user={user} />\n            </Route>\n          ))}\n          {user &&\n            privatRoutes.map(({ path, Component }) => (\n              <Route key={path} path={path} exact={true}>\n                <Component user={user} />\n              </Route>\n            ))}\n          <Redirect to={MAIN_ROUTE} />\n        </Switch>\n        {useMemo(() => footerContent && <Footer />, [footerContent])}\n        {useMemo(\n          () => (\n            <MenuModal user={user} />\n          ),\n          [user]\n        )}\n        {useMemo(\n          () => (\n            <LoginModal user={user} login={login} />\n          ),\n          [user]\n        )}\n        {useMemo(\n          () => (\n            <DashboardModal\n              user={user}\n              login={login}\n              firebaseAuth={firebaseAuth}\n            />\n          ),\n          [user]\n        )}\n        {useMemo(\n          () => (\n            <MessageModal />\n          ),\n          []\n        )}\n        {/* </BrowserRouter> */}\n      </HashRouter>\n    </AppContext.Provider>\n  )\n}\n\nexport default AppRouter\n","/Users/andrii/projects/35-fit/src/components/CommunityArticle.tsx",[],"/Users/andrii/projects/35-fit/src/components/Footer.tsx",[],"/Users/andrii/projects/35-fit/src/components/HeaderBanner.tsx",["179"],"import React, { useEffect } from 'react'\nimport { useTranslation } from 'react-i18next'\nimport styled from 'styled-components'\nimport { useAppDispatch } from 'utils/hooks'\nimport { unsetAllModal } from 'utils/modal.slice'\nimport { CHTDCLProps } from 'types/interface'\n\nconst Div = styled.div`\n  section {\n    padding-top: 144px;\n  }\n\n  h1 {\n    font-size: 36px;\n    margin-bottom: 40px;\n  }\n\n  p {\n    max-width: 400px;\n    font-size: 18px;\n    padding-bottom: 70px;\n  }\n\n  &.main h1 {\n    margin-bottom: 120px;\n    text-transform: uppercase;\n    text-align: center;\n    @media (min-width: 993px) {\n      margin-bottom: 210px;\n    }\n  }\n\n  @media (max-width: 992px) {\n    section {\n      h3 {\n        font-size: 16px;\n      }\n    }\n  }\n\n  @media (min-width: 993px) {\n    section {\n      padding-top: 170px;\n      h1 {\n        font-size: 48px;\n      }\n      h3 {\n        padding-bottom: 140px;\n      }\n    }\n  }\n`\n\nexport const HeaderBanner = ({\n  children,\n  className,\n  title,\n  descript,\n}: CHTDCLProps) => {\n  const dispatch = useAppDispatch(),\n    { t } = useTranslation()\n\n  const keyDownHandler = (e: { key: string }) => {\n    e.key === 'Escape' && dispatch(unsetAllModal())\n  }\n\n  useEffect(() => {\n    window.scrollTo(0, 0)\n    document.addEventListener('keydown', (e) => keyDownHandler(e))\n    return () => {\n      document.removeEventListener('keydown', keyDownHandler)\n      dispatch(unsetAllModal())\n    }\n  }, [])\n\n  return (\n    <Div className={`header_banner ${className}`}>\n      <section className=\"section\">\n        <h1 className=\"b900 blue\">{t(title)}</h1>\n        {descript && <p className=\"grey\">{t(descript)}</p>}\n        {children}\n      </section>\n    </Div>\n  )\n}\n","/Users/andrii/projects/35-fit/src/components/HeaderNavigate.tsx",[],"/Users/andrii/projects/35-fit/src/components/ImageDescription.tsx",[],"/Users/andrii/projects/35-fit/src/components/InstaImages.tsx",[],"/Users/andrii/projects/35-fit/src/components/Language.tsx",[],"/Users/andrii/projects/35-fit/src/components/NavigationLinks.tsx",[],"/Users/andrii/projects/35-fit/src/components/Spinner.tsx",[],"/Users/andrii/projects/35-fit/src/components/User.tsx",[],"/Users/andrii/projects/35-fit/src/img/icons.tsx",[],"/Users/andrii/projects/35-fit/src/index.tsx",[],"/Users/andrii/projects/35-fit/src/modals/DashboardModal.tsx",["180"],"import React, { useMemo } from 'react'\nimport { useTranslation } from 'react-i18next'\nimport styled from 'styled-components'\nimport { useAppDispatch, useAppSelector } from 'utils/hooks'\nimport { selectDashModalActive, unsetDashModal } from '../utils/modal.slice'\nimport { BaseDiv } from './MenuModal'\nimport { NavigationLinks } from 'components/NavigationLinks'\nimport {